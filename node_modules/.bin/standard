#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules/standard/bin/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules/standard/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules/standard/bin/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules/standard/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/standard@17.1.2/node_modules:/mnt/d/projects/nodeProjects/rds/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../standard/bin/cmd.cjs" "$@"
else
  exec node  "$basedir/../standard/bin/cmd.cjs" "$@"
fi
